# WordPress Plugin .gitignore

# Node.js dependencies and logs
/node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
/package-lock.json
/yarn.lock

# Composer dependencies and lock
/vendor/
composer.lock

# Build outputs and compiled assets
/dist/
/build/
*.min.js
*.min.css
*.map

# Environment and sensitive files
.env
.env.local
.env.*

# Log and debug files
*.log
debug.log
error_log
wp-content/debug.log

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
Thumbs.db
desktop.ini
$RECYCLE.BIN/

# Cache and temporary files
cache/
tmp/
temp/

# Backup files and directories
*.bak
*.backup
backup/
backups/

# Archives
*.zip
*.tar.gz
*.rar
*.7z

# Test artifacts
/tests/coverage/
phpunit.xml
.phpunit.result.cache

# Ignore plugin-specific local config
auto-nulis-config.json
auto-nulis-backup/
